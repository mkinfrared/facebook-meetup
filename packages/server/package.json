{
  "name": "@facebook-metup/server",
  "version": "0.0.1",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Mikhail Korotkov",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build-sass && npm run build-ts && npm run copy-static-assets",
    "serve": "node build/server.js",
    "watch-node": "nodemon build/server.js",
    "start": "cross-env NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "ttsc",
    "watch-ts": "ttsc -w",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "lusca": "^1.6.1",
    "passport-facebook": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.14",
    "@types/lusca": "^1.6.0",
    "@types/node": "^12.0.10",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "jest": "24.7.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "open-cli": "^5.0.0",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.7",
    "typescript": "^3.5.3",
    "typescript-transform-paths": "^1.1.4"
  }
}
